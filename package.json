{
    "name": "myx",
    "version": "1.0.0",
    "description": "",
    "types": "dist/types/helpers/index.d.ts",
    "author": "themis",
    "main": "dist/helpers/index.js",
    "scripts": {
        "format": "prettier --check \"./**/*.ts\" README.md",
        "format:fix": "prettier --write \"./**/*.ts\" README.md",
        "sol:fix": "prettier --write \"./**/*.sol\" README.md",
        "generate-types": "hardhat typechain",
        "clean": "rm -rf artifacts/ types/ cache/",
        "compile": "SKIP_LOAD=true npx hardhat compile",
        "test": "npx hardhat test",
        "test:pool:liquidity": "npm run compile && npx hardhat test test/__setup.spec.ts test/pool-liquidity.spec.ts",
        "test:trade:pnl": "npm run compile && npx hardhat test test/__setup.spec.ts test/trade-pnl.spec.ts",
        "test:trade:fee": "npm run compile && npx hardhat test test/__setup.spec.ts test/trade-fee.spec.ts",
        "deploy": "npx hardhat deploy",
        "coverage": "npx hardhat coverage --testfiles 'test/*.spec.ts'",
        "sizer": "hardhat size-contracts"
    },
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.2.3",
        "@nomiclabs/hardhat-web3": "^2.0.0",
        "@types/mocha": "^10.0.1",
        "@types/sinon-chai": "^3.2.9",
        "@typescript-eslint/eslint-plugin": "4.29.2",
        "@typescript-eslint/parser": "4.29.2",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.24.1",
        "eslint-plugin-no-only-tests": "3.0.0",
        "eslint-plugin-prettier": "3.4.0",
        "ethereum-waffle": "^4.0.10",
        "hardhat": "^2.17.0",
        "hardhat-deploy": "^0.11.34",
        "hardhat-log-remover": "^2.0.2",
        "mocha": "^10.2.0",
        "mocha-chai-jest-snapshot": "^1.1.4",
        "prettier": "2.4.1",
        "ts-node": "^10.9.1",
        "web3": "^4.1.1"
    },
    "dependencies": {
        "@chainlink/contracts": "^0.6.1",
        "@matterlabs/hardhat-zksync-deploy": "^0.6.3",
        "@matterlabs/hardhat-zksync-solc": "^0.3.17",
        "@matterlabs/hardhat-zksync-verify": "^0.1.7",
        "@nomiclabs/hardhat-etherscan": "^3.1.7",
