        "abi-decoder": "^2.4.0",
        "csv-parse": "^5.4.0",
        "decimal.js": "^10.4.3",
        "dotenv": "^16.0.3",
        "hardhat-abi-exporter": "^2.10.1",
        "hardhat-contract-sizer": "^2.10.0",
        "hardhat-gas-reporter": "^1.0.9",
        "keccak256": "^1.0.6",
        "merkletreejs": "^0.3.10",
